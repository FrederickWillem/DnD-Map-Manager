<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="100.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InitiativeListController">
   <children>
      <ListView fx:id="listview" minWidth="100.0" style="-fx-opacity: 1.0;" VBox.vgrow="ALWAYS" />
      <ButtonBar fx:id="bar" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="btnClear" mnemonicParsing="false" text="Clear" />
            <Button fx:id="btnRemove" mnemonicParsing="false" text="Remove" />
            <Button fx:id="btnNext" mnemonicParsing="false" text="Next" />
            <Button fx:id="btnPrev" mnemonicParsing="false" text="Previous" />
        </buttons>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
